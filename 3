#include <stdio.h>

// Функция для определения знака числа
int sign(int x) {
    return (x > 0) - (x < 0); // Если x > 0, возвращает 1; если x < 0, возвращает -1; иначе 0
}

int main() {
    // Переменные для хранения трех чисел
    int a, b, c;

    // Запрашиваем три числа у пользователя
    printf("Введите три числа через пробел: ");
    scanf("%d %d %d", &a, &b, &c);

    // Вычисляем знак произведения
    int result = sign(a) * sign(b) * sign(c);

    // Выводим результат
    printf("%d\n", result);

    return 0;
}
